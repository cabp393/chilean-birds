---
import { tagList } from '../components/tagList.astro'
import TagPageComp from '../components/TagPageComp.astro'

export function getStaticPaths() {
  const filter = tagList.filter(t => t.tag !== '/')

  return filter.map(tagItem => ({
    params: {
      tagPage: tagItem.tag,
    },
    props: {
      tagItem,
    },
  }))
}
const { tagItem } = Astro.props
---

<TagPageComp pTag={tagItem.tag} />
